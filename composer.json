{
  "name": "zynga/framework",
  "description": "Zynga Hacklang Framework - Collection of hack classes that enable us to utilize strict mode within zynga.",
  "type": "library",
  "keywords": [
    "hhvm",
    "hack",
    "framework"
  ],
  "require": {
    "hhvm": "^3.12",
    "zynga/phpunit": "v2.0.9",
    "zynga/phpunit-hhi": "v1.0.2"
  },
  "bin": [
    "bin/curl-upload-file.sh"
  ],
  "autoload": {
    "psr-4": {
      "Zynga\\Framework\\": "src/Zynga/Framework/"
    }
  },
  "repositories": [
    {
      "packagist.org": false
    },
    {
      "type": "vcs",
      "url": "git@github.com:Zynga/zynga-hhvm-phpunit-hhi.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:Zynga/zynga-hhvm-phpunit.git"
    }
  ]
}
